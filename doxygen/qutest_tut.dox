     expect "%timestamp LEDDRIVER_TURNON 0x0180"
     expect "%timestamp Trg-Done QS_RX_COMMAND"

     ~ ~ ~

     #----------
     test "LED memory is not readable" -noreset
     current_obj AP virtualLeds
     poke 0 2 [binary format s 0xFFFF]
