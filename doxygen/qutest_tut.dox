         QS_FUN_DICTIONARY(&Flash_Destroy);
         QS_FUN_DICTIONARY(&Flash_Write);

         /* initialize the mocks... */
 [7]     MockIO_Init();

         return QF_run(); /* run the tests */
     }

     /*--------------------------------------------------------------------------*/
     void QS_onTestSetup(void) {
 [8]     Flash_Create();
     }
     /*..........................................................................*/
     void QS_onTestTeardown(void) {
 [9]     Flash_Destroy();
