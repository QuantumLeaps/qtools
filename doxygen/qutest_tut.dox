     void QS_onCommand(uint8_t cmdId,
                       uint32_t param1, uint32_t param2, uint32_t param3)
     {
         (void)param2;
         (void)param3;

         switch (cmdId) {
[10]         case FLASH_WRITE: {
[11]             int ret = Flash_Write((ioAddress)param1, (ioData)param2);
[12]             QS_BEGIN(FLASH_WRITE, (void *)0) /* user-specific record */
[13]                QS_I32(0, ret);
                 QS_END()
